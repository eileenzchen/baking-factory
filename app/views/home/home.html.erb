<div id="main_content">

	<% if current_user.nil? %>
		<%= render :partial => 'general_home' %>
	<% elsif current_user.role?(:admin) %>
		<%= render :partial => 'admin_home' %>
	<% elsif current_user.role?(:customer) %>
		<%= render :partial => 'customer_home' %>
	<% elsif current_user.role?(:shipper) %>
		<%= render :partial => 'shipper_home' %>
	<% else %>
		<%= render :partial => 'baker_home' %>
	<% end %>

</div>
	

